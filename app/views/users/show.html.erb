                  <div class="col-md-6 offset-md-1">
                    <div class="nk-counter-2">
                      <div>
                        <% if @lastJOBKills == [] %>
                          <div class="nk-counter-title h4" style="font-size: 32px;"> <%= @info[:charname16] %> is shy of fighting </div>
                        <% else %>
                          <% @lastJOBKills.each do |kill| %>
                            <%
                              @victim = @DB[:_char].where(:charid => kill[:deadcharid]).all.last
                              @slayer = @DB[:_char].where(:charid => kill[:charid]).all.last
                            %>
                            <div class="nk-counter-title h4" style="font-size: 32px;">
                              <% if  @victim[:charname16] == @info[:charname16] %>
                                <%= @info[:charname16] %>
                                <small> killed by </small>
                                <%= @slayer[:nickname16]  %>
                              <% else %>
                                <%= @info[:charname16] %>
                                <small> has killed </small>
                                <%= @victim[:nickname16]  %>
                              <% end %>
                            </div>
                            <div class="text-center"> <i class="far fa-clock" style="padding-right: 1%"></i> <%= time_ago_in_words(kill[:eventtime]) %> </div>
                          <% end %>
                        <% end %>
                      </div>
                    </div>
